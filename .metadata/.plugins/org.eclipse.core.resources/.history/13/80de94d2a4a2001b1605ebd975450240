from tkinter import Tk
class Window:
    root = Tk()
    
    def __init__(self):
        
        w, h = self.root.winfo_screenwidth(),self.root.winfo_screenheight()
        self.root.state("zoomed")
        elf.root.geometry("%dx%d+0+0" % (w,h))
        self.root.title("Registration Form")
        